
    <div id="content">
      <div class="data-form">
        <form class="form-signin" action="/products/create" method="POST" encType="multipart/form-data">
          <h2 class="form-signin-heading">Update Product Inventory</h2>
          <label for="inputProductName">Product Name</label>
          <input type="text" name="name" id="inputProductName" class="form-control" placeholder="Product Name" autofocus required>
          <label for="inputDescription">Description</label>
          <input type="text" name="description" id="inputDescription" class="form-control" placeholder="Product Description" required>
          <label for="inputPrice">Price</label>
          <input type="number" step="0.01" name="price" id="inputPrice" class="form-control" placeholder="Price" required>
          <label for="fileInput">File Input</label>
          <input type="file" name="imageURL" id="fileInput" accept="image/*" required>
          <br />
          <button class="btn btn-lg btn-primary btn-block" type="submit">Submit</button>
        </form>
      </div>
    </div>

