
    <div id="content">
        <div class="data-form">
          <form class="form-signin" method="POST" action="<%=`/gardens/${garden._id}/update?_method=PUT`%>" encType="multipart/form-data">
            <h2>Edit Garden:</h2>
            <label for="inputGardenID">GardenID</label>
            <input type="text" name="GardenID" id="GardenID" value="<%=garden.GardenID%>" class="form-control" placeholder="GardenID" autofocus required>
            <label for="inputLocation">Location</label>
            <input type="text" name="Location" id="Location" value="<%=garden.Location%>" class="form-control" placeholder="Location" autofocus required>
            <label for="inputLocation">Description</label>
            <input type="text" name="Description" id="Description" value="<%=garden.Description%>" class="form-control" placeholder="Description" autofocusrequired>
            <label for="fileInput">File Input</label>
            <input type="file" name="imageURL" id="fileInput" value="<%=garden.imageURL%>" accept="image/*" required>
            <br />
            <button class="btn btn-lg btn-primary btn-block" type="submit">Update</button>
          </form>
        </div>
      </div>
 
